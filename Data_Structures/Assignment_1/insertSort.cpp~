#include<iostream>
#include<cstdio>
using namespace std;

void insertSort(int a[200],int size){
  for(int i = 0; i < size-1; i++){
    if(a[i+1] < a[i]){
      int j = i+1;
      while(j >=  0 && a[j] < a[j-1]){
	int temp = a[j];
	a[j] = a[j-1];
	a[j-1] = temp;
	j--;
      }
    }
  }
  for(int j = 0; j < size; j++){
    cout << array[j] << " " ;}
  cout << endl;
}

int main(){
  int n = 0;
  while(n < 200 || n == 0){
  	  
  cout << "Enter the number of inputs (less than 200)" << endl;
  cin >> n;
  }
  cout << n << endl;
  return 0;
  }/*
do{
    cin >> n;
  }while{
    n <= 200;}*/
